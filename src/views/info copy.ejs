    }

    .bayarpajakku-font {
      font-family: "Lobster", sans-serif;
      font-weight: 400;
      font-style: normal;
    }

    .animate-slide-in {
      animation: slideIn 0.5s ease-out;
    }

    .animate-fade-in {
      animation: fadeIn 0.8s ease-out;
    }

    @keyframes slideIn {
      from {
        opacity: 0;
        transform: translateY(20px);
      }

      to {
        opacity: 1;
        transform: translateY(0);
      }
    }

    @keyframes fadeIn {
      from {
        opacity: 0;
      }

      to {
        opacity: 1;
      }
    }

    /* Horizontal Connecting Line for Desktop */
    .timeline-container::before {
      content: "";
      position: absolute;
      top: 1.75rem;
      /* Center of circular marker (3.5rem / 2) */
      left: 1.75rem;
      /* Center of first marker */
      right: 1.75rem;
      /* Center of last marker */
      height: 2px;
      background: linear-gradient(to right, #005d69, #e5e7eb);
      z-index: 0;
      display: none;
    }

    @media (min-width: 768px) {
      .timeline-container::before {
        display: block;
      }
    }

    /* Rotate Chevron on FAQ Open */
    details[open] .chevron {
      transform: rotate(180deg);
      transition: transform 0.3s ease;
    }

    .chevron {
      transition: transform 0.3s ease;
    }

    /* Gradient Underline for Headings */
    .heading-underline::after {
      content: "";
      display: block;
      width: 60px;
      height: 3px;
      background: linear-gradient(to right, #005d69, #f97316);
      margin: 0.5rem auto;
    }
  </style>
</head>

