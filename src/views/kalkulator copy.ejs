    <script>
      function hitung() {
        // Ambil nilai input
        const gaji = parseFloat(document.getElementById('gaji').value) || 0;
        const tunjanganPph = parseFloat(document.getElementById('tunjangan-pph').value) || 0;
        const tunjanganLain = parseFloat(document.getElementById('tunjangan-lain').value) || 0;
        const honorarium = parseFloat(document.getElementById('honorarium').value) || 0;
        const premiAsuransi = parseFloat(document.getElementById('premi-asuransi').value) || 0;
        const natura = parseFloat(document.getElementById('natura').value) || 0;
        const bonus = parseFloat(document.getElementById('bonus').value) || 0;

        // Hitung Penghasilan Bruto
        const penghasilanBruto = gaji + tunjanganPph + tunjanganLain + honorarium + premiAsuransi + natura + bonus;
        document.getElementById('penghasilanBruto').value = penghasilanBruto.toFixed(0);

        // Ambil periode dari
        const masaDari = document.getElementById('masaDari').value;
        const months = [
          'Januari', 'Februari', 'Maret', 'April', 'Mei', 'Juni',
          'Juli', 'Agustus', 'September', 'Oktober', 'November', 'Desember'
        ];
        const startMonthIndex = months.indexOf(masaDari);
        const endMonthIndex = months.indexOf('Desember');
        const bulanPeriode = endMonthIndex - startMonthIndex + 1;

        // Hitung Biaya Jabatan (5% dari penghasilan bruto, maksimal 500,000 per bulan atau 6,000,000 per tahun)
        const biayaJabatanBulanan = Math.min(penghasilanBruto * 0.05, 500000);
        const biayaJabatan = Math.min(biayaJabatanBulanan * bulanPeriode, 6000000);
        document.getElementById('biayaJabatan').value = biayaJabatan.toFixed(0);

        // Update Penghasilan Bruto Setahun (disesuaikan dengan periode)
        const penghasilanBrutoSetahun = penghasilanBruto * (12 / bulanPeriode);
        document.getElementById('penghasilan-bruto-setahun').value = penghasilanBrutoSetahun.toFixed(0);

        // Update Biaya Jabatan Setahun
        const biayaJabatanSetahun = biayaJabatan * (12 / bulanPeriode);
        document.getElementById('biaya-jabatan-setahun').value = biayaJabatanSetahun.toFixed(0);
      }
    </script>

